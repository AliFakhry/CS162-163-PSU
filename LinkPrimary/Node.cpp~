#include <iostream>
#include <cstring>
#include "Node.h"
#include "Student.h"

using namespace std;
Node::Node() {
  student = NULL;
  next = NULL;
}
Node::~Node() {
  delete &student;
  next = NULL;
}
void Node::setStudent(Student* newStudent) {
  student = newStudent;
}
Student* Node::getStudent() {
  return student;
}
void Node::setNext(Node* newNext) {
  next = newnext;
}
Node* Node::getNext() {
  return next;
}
