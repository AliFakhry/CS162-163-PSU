#ifndef STUDENT_H
#define STUDENT_H

#include <iostream>
#include <cstring>

using namespace std;

class Student{
 public: 
  Student(char* setFirstName, char* setLastName, int setID, double setGPA);
  ~Student();
  char* getFirstName();
  char* getLastName();
  double getGPA();
  int getID();
  char* lastName;
  char* firstName;
  int ID;
  double GPA;


};
#endif
